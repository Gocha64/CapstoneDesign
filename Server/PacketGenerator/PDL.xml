<?xml version="1.0" encoding="utf-8" ?>
<PDL>
	<packet name="C_Request_Name_input">
		<String name="name"/>
		<String name="Uid"/>
	</packet>
	<packet name="S_Reply_Name_input">
		<byte name ="reply"/>
	</packet>
	<packet name="C_Request_Load_Star">
		<String name ="UId"/>
	</packet>
	<packet name="S_Challenge_Load_Star">
		<list name="StageStar">
			<byte name="stageId"/>
			<byte name="numberOfStars"/>
		</list>
	</packet>
	<packet name="C_Request_Challenge_MyPage">
		<byte name ="reply"/>
	</packet>
	<packet name="S_Challenge_MyPage">
		<string name="UId"/>
	</packet>
	<packet name="C_Request_Challenge_Top30Rank">
	</packet>
	<packet name="S_Challenge_Top30Rank">
		<list name="Rank">
			<string name="UId"/>
			<byte name="ranking"/>
			<byte name="totalStars"/>
		</list>
	</packet>
	<packet name="C_ChallengeUpdateStars">
		<string name="UId"/>
		<byte name="stageId"/>
		<byte name="numberOfStars"/>
	</packet>
</PDL>