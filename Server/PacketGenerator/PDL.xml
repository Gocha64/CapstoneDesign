<?xml version="1.0" encoding="utf-8" ?>
<PDL>
	<packet name="S_ChallengeTotalStars">
		<byte name="TotalStars"/>
	</packet>
	<packet name="S_ChallengeCheckMyRanking">
		<int name="ranking"/>
	</packet>
	<packet name="S_GetStudyMaxStage">
		<byte name="maxStage"/>
	</packet>
	<packet name="S_LoadChallengeStar">
		<list name="StageStar">
			<byte name="stageId"/>
			<byte name="numberOfStars"/>
		</list>
	</packet>
	<packet name="S_ChallengeTop30">
		<list name="Rank">
			<string name="UId"/>
			<byte name="ranking"/>
			<byte name="totalStars"/>
		</list>
	</packet>
	<packet name="C_ChallengeUpdateStars">
		<string name="UId"/>
		<byte name="stageId"/>
		<byte name="numberOfStars"/>
	</packet>
	<packet name="C_RequestMyChallengeProgress">
		<string name="UId"/>
	</packet>
	<packet name="C_RequestMyChallengeRanking">
		<string name="UId"/>
	</packet>
	<packet name="C_OpenNextStudyStage">
		<string name="UId"/>
		<byte name="stageId"/>
	</packet>
	<packet name="C_RequestTotalStars">
		<string name="UId"/>
	</packet>
	<packet name="C_RequestStudyProgress">
		<string name="UId"/>
	</packet>
	<packet name="C_RequestChallengeTop30">
		<string name="UId"/>
	</packet>
	
</PDL>